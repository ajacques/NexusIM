<?xml version="1.0" encoding="utf-8"?><Database Name="ChatHistory" xmlns="http://schemas.microsoft.com/linqtosql/dbml/2007">
  <Table Name="PrivateMessages">
    <Type Name="PrivateMessage">
      <Column Name="Timestamp" Type="System.DateTime" DbType="DateTime NOT NULL" IsPrimaryKey="true" CanBeNull="false" />
      <Column Name="MessageBody" Type="System.String" DbType="NVarChar(4000) NOT NULL" CanBeNull="false" />
      <Column Name="Sender" Type="System.String" DbType="NVarChar(64) NOT NULL" CanBeNull="false" />
      <Column Name="Recipient" Type="System.String" DbType="NVarChar(64) NOT NULL" CanBeNull="false" />
    </Type>
  </Table>
</Database>